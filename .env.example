# Rapras認証設定
# RaprasサイトのユーザーID
RAPRAS_USERNAME=your_rapras_username_here

# Raprasサイトのパスワード
RAPRAS_PASSWORD=your_rapras_password_here

# Yahoo Auctions認証設定
# Yahoo Auctionsログイン用の電話番号（ハイフンなし、例: 09012345678）
YAHOO_PHONE_NUMBER=your_phone_number_here

# プロキシサーバー設定
# プロキシサーバーのURL（例: http://164.70.96.2:3128）
PROXY_URL=http://your_proxy_server:port

# プロキシBASIC認証のユーザー名
PROXY_USERNAME=your_proxy_username_here

# プロキシBASIC認証のパスワード
PROXY_PASSWORD=your_proxy_password_here
